{
  "local": {
    "userAID": "@uuid()@",
    "userAName": "@letter(4)@",
    "userBID": "@uuid()@",
    "userBName": "@letter(4)@",
    "bookA1ID":"@uuid()@",
    "bookA1Name": "a1",
    "bookA2ID":"@uuid()@",
    "bookA2Name": "a2",
    "bookB1ID":"@uuid()@",
    "bookB1Name": "b1",
    "bookB2ID":"@uuid()@",
    "bookB2Name": "b2"
  },
  "table" :  {
    "user": [
      {
        "id": "@local.userA@",
        "name": "@local.userAName@",
        "is_super": false
      },
      {
        "id": "@local.userB@",
        "name": "@local.userBName@",
        "is_super": false
      }
    ],
    "book": [
      {
        "id": "@local.bookA1ID@",
        "name": "@local.bookA1Name@",
        "user_id": "@local.userAID"
      },
      {
        "id": "@local.bookA2ID@",
        "name": "@local.bookA2Name@",
        "user_id": "@local.userAID"
      },
      {
        "id": "@local.bookB1ID@",
        "name": "@local.bookB1Name@",
        "user_id": "@local.userBID"
      },
      {
        "id": "@local.bookB2ID@",
        "name": "@local.bookB2Name@",
        "user_id": "@local.userBID"
      }
    ]
  }
}